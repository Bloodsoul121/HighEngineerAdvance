<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blood.highengineeradvance">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.customview.CustomViewActivity" />
        <activity android:name=".ui.customview.PointAnimaterActivity" />
        <activity android:name=".ui.customview.ImitateActivity"
            android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".ui.customview.CustomLayoutActivity"/>
    </application>

</manifest>